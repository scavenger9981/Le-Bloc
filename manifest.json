{
  "name": "Shared 5-sec YouTube Gate",
  "description": "After 5 global seconds on YouTube, every tab is blocked together.",
  "version": "4.0",
  "manifest_version": 3,
  "permissions": ["tabs", "storage"],
  "host_permissions": ["*://*.youtube.com/*"],
  "background": { "service_worker": "background.js" },
  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*"],
      "js": ["content.js"],
      "run_at": "document_start"
    }
  ]
}